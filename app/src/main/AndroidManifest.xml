<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="co.tiagoaguiar.numerodasorte">

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.Design.NoActionBar"
        tools:ignore="HardcodedDebugMode">
        <activity android:name="co.tiagoaguiar.numerodasorte.LotoFacilActivity"></activity>
        <activity android:name="co.tiagoaguiar.numerodasorte.QuinaActivity" />
        <activity android:name="co.tiagoaguiar.numerodasorte.MegaActivity" />
        <activity android:name="co.tiagoaguiar.numerodasorte.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>